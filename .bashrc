############### SELF-DEFINED ALIASES ###############
# Set coding space directory
alias code='cd ~/Dropbox/it/'

# Competitive programming space
alias CP='code; cd ./CP'

# Sandbox space
alias sandbox='code; cd ./sandbox'
alias sb='sandbox'

# Replace the default 'rm' command in linux with 'rm' in trash-cli package. This will move deleted files to trash bin instead of permanent delete.
alias rm=trash

# Open directory at ...
alias odir='nautilus'

# Quick C++ code compilation
alias cpl='g++ -g -o run'

# Set --no-gui option to default in Octave
alias octave='octave --no-gui'

# Shortcut for 'clear'
alias cc='clear'
alias clc='clear'
alias cls='clear'
alias clera='clear'
